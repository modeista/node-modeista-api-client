{
  "author": "Martin Wawrusch <martin@wawrusch.com> (http://martinatsunset.com)",
  "name": "modeista-api-client",
  "description": "A client to connect to the modeista API.",
  "version": "0.0.8",
  "main" : "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/modeista/node-modeista-api-client.git"
  },
  "bin" : {
  },
  "directories" : {
    "lib" : "./lib",
    "test" : "./test"
  },
  "keywords" : [
    "modeista","api client"
  ],
  "scripts": {
    "prepublish" : "node_modules/.bin/coffee -c -o lib src",
    "watch" : "node_modules/.bin/coffee -c -w  -o lib src",
    "test": "node_modules/.bin/coffee -c -o lib src;NODE_ENV=test node_modules/.bin/mocha --timeout 10000 -R spec test/*.coffee"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies": {
    "underscore": "1.4.4",
    "request": "2.21.0",
    "pkginfo" : "0.3.0"
  },
  "devDependencies": {
    "async": "0.2.9",
    "coffee-script": "1.6.3",
    "should": "1.2.2",
    "mocha": "1.10.0"

  },
  "bugs": { "url": "http://github.com/modeista/node-modeista-api-client/issues" },
  "licenses": [
    { "type": "MIT",
      "url": "http://github.com/modeista/node-modeista-api-client/raw/master/LICENSE"
    }
  ]
  
}
